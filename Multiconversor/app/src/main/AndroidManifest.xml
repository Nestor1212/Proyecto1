<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.multiconversor" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_aplicacion"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".principalmulticonversor"
            android:label="@string/title_activity_principalmulticonversor" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".calculadora"
            android:label="@string/title_activity_calculadora" >
        </activity>
        <activity
            android:name=".temperatura"
            android:label="@string/title_activity_temperatura" >
        </activity>
        <activity
            android:name=".longitud"
            android:label="@string/title_activity_longitud" >
        </activity>
        <activity
            android:name=".divisas"
            android:label="@string/title_activity_divisas" >
        </activity>
    </application>

</manifest>
